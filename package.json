{
  "name": "cypressqce",
  "version": "1.0.0",
  "description": "Quick Count Election Functional Test With Cypress and Cucumber",
  "main": "index.js",
  "scripts": {
    "cypress:run": "cypress run",
    "generate:report": "node cucumber-html-report.js",
    "verify": "npx cypress run & npm run generate:report"
  },
  "keywords": [
    "Cypress",
    "BDD",
    "Cucumber"
  ],
  "author": "arifldhewo",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "cypress": "^13.6.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-xpath": "^2.0.1",
    "multiple-cucumber-html-reporter": "^3.5.0",
    "neat-csv": "^7.0.0",
    "npm-run-all": "^4.1.5"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": false,
    "stepDefinitions": "cypress/e2e/qce",
    "filterSpecs": true,
    "omitFiltered": true,
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/reports/json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  }
}
